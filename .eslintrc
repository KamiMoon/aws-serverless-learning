{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  },
  "plugins": ["jest", "prettier"]
}
